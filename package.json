    "@types/react": "^16.9.36",
    "jest": "^26.0.1",
    "mustache": "^4.0.1",
    "react": "^16.13.1",
    "rollup": "^2.15.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
